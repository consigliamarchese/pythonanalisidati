     "execution_count": 13,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "# Filtriamo solo i paesi reali\n",
    "# Gli aggregati hanno continent = NaN\n",
    "solo_paesi = covid_19[covid_19['continent'].notna()]\n",
    "\n",
    "# Per ogni paese prendiamo il totale finale dei casi\n",
    "# total_cases è cumulativo → il valore massimo è il totale dall'inizio pandemia\n",
    "ultimo_per_paese = (\n",
    "    solo_paesi\n",
    "    .groupby(['location', 'continent'])['total_cases']\n",
    "    .max()\n",
    "    .reset_index()\n",
    ")\n",
    "\n",
    "# Sommiamo i casi per continente\n",
    "totali_per_continente = (\n",
    "    ultimo_per_paese\n",
    "    .groupby('continent')['total_cases']\n",
    "    .sum()\n",
    ")\n",
    "\n",
    "# Calcoliamo il totale mondiale\n",
    "totale_mondiale = totali_per_continente.sum()\n",
    "\n",
    "# Calcoliamo la percentuale sul totale mondiale\n",
    "percentuali = (totali_per_continente / totale_mondiale) * 100\n",
    "\n",
    "# Creiamo il DataFrame finale\n",
    "risultato = pd.DataFrame({\n",
    "    'Casi_totali': totali_per_continente,\n",
    "    'Percentuale_sul_totale_mondiale': percentuali\n",
    "})\n",
    "risultato['Percentuale_sul_totale_mondiale'] = (risultato['Percentuale_sul_totale_mondiale'].round(2).astype(str) + '%'\n",
    ")\n",
    "\n",
    "\n",
    "# Ordiniamo per numero di casi decrescente\n",
    "risultato = risultato.sort_values(by='Casi_totali', ascending=False)\n",
    "\n",
    "risultato\n"
   ]
  },
