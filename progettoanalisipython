   ],
   "source": [
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# 1️⃣ Filtraggio per paesi e anno 2021\n",
    "covid_2021 = covid_19.loc[\n",
    "    (covid_19['location'].isin(['Italy', 'Germany', 'France', 'Spain'])) &\n",
    "    (covid_19['anno'] == 2021)\n",
    "]  # filtriamo per paesi e anno di riferimento\n",
    "\n",
    "# 2️⃣ Somma dei pazienti ospedalizzati per ciascun paese\n",
    "somma_pazienti = covid_2021.groupby('location')['hosp_patients'].sum()\n",
    "print(\"Somma dei pazienti ospedalizzati per paese:\\n\", somma_pazienti)\n",
    "\n",
    "# 3️⃣ Controllo dei valori nulli\n",
    "null_count = covid_2021[['location', 'hosp_patients']].isnull().sum()\n",
    "print(\"\\nValori nulli per colonna:\\n\", null_count)\n",
    "\n",
    "# 4️⃣ Mostra tutte le righe con valori nulli\n",
    "dati_nulli = covid_2021[covid_2021['hosp_patients'].isnull()]\n",
    "print(\"\\nRighe con valori nulli in hosp_patients:\\n\", dati_nulli[['location', 'hosp_patients', 'date']])\n",
    "\n",
    "# 5️⃣ Grafico a barre della somma dei pazienti ospedalizzati\n",
    "somma_pazienti.plot(kind='bar', color=['green','blue','red','orange'])\n",
    "plt.title(\"Somma dei pazienti ospedalizzati nel 2021\")\n",
    "plt.ylabel(\"Numero di pazienti\")\n",
    "plt.xlabel(\"Paese\")\n",
    "plt.xticks(rotation=0)\n",
    "plt.show()\n"
   ]
